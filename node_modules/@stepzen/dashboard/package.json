{
  "name": "@stepzen/dashboard",
  "version": "0.1.37",
  "author": "Darren Waddell <darren@stepzen.com>",
  "license": "MIT",
  "homepage": "https://stepzen.com",
  "files": [
    "/src/server",
    "/src/ui/build"
  ],
  "engines": {
    "node": ">=14.0.1",
    "npm": ">=6.14"
  },
  "main": "src/server",
  "scripts": {
    "dev:server": "nodemon src/dev --watch src/dev --watch src/server",
    "dev:ui": "cd src/ui && REACT_APP_IS_DEVELOPMENT=1 npm start",
    "prepack": "cd src/ui && npm run build",
    "prepare": "husky install && cd src/ui && npm install",
    "start": "concurrently \"npm run dev:server\" \"npm run dev:ui\"",
    "test": "exit 0",
    "posttest": "prettier . --check"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "cross-spawn": "^7.0.3",
    "debug": "^4.3.3",
    "express": "^4.17.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "node-fetch": "^2.6.7",
    "open": "^8.4.0",
    "prettier": "^2.5.1"
  },
  "lint-staged": {
    "*.{ts,js,css,scss,md,yaml,json}": "prettier --write"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.7"
  }
}
