{
  "name": "@stepzen/transpiler",
  "description": "The StepZen transpiler",
  "version": "0.0.38",
  "author": "Darren Waddell <darren@stepzen.com>",
  "license": "MIT",
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=14.0.1",
    "npm": ">=6.14"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "rm -rf lib && tsc -b && nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "prettier . --check"
  },
  "dependencies": {
    "@graphql-tools/merge": "^6.2.17",
    "debug": "^4.3.3",
    "dotenv": "^10.0.0",
    "fs-extra": "^9.1.0",
    "glob": "^7.2.0",
    "graphql": "^15.8.0",
    "inquirer": "^8.2.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "prettier": "^2.5.1",
    "shelljs": "^0.8.5",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.11",
    "@types/glob": "^7.2.0",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.2",
    "@types/mock-fs": "^4.13.0",
    "@types/node-fetch": "^2.6.1",
    "@types/prettier": "^2.4.4",
    "@types/shelljs": "^0.8.9",
    "chai": "^4.3.4",
    "fancy-test": "^1.4.10",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "mocha": "^9.0.1",
    "mock-fs": "^4.13.0",
    "node-ts": "^5.1.1",
    "nyc": "^14.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{ts,js,css,md,yaml,json}": "prettier --write"
  }
}
