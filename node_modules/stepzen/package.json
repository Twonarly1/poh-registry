{
  "name": "stepzen",
  "description": "The StepZen CLI",
  "version": "0.15.0",
  "license": "MIT",
  "author": "Darren Waddell <darren@stepzen.com>",
  "contributors": [
    "Lucia Cerchie <lucia@stepzen.com"
  ],
  "homepage": "https://stepzen.com",
  "bin": {
    "stepzen": "./bin/run"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=14.0",
    "npm": ">=6.14"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "graphql"
  ],
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.18.3",
    "@oclif/core": "1.7.0",
    "@oclif/errors": "1.3.5",
    "@oclif/plugin-help": "^5.1.12",
    "@stepzen/dashboard": "0.1.37",
    "@stepzen/sdk": "0.11.0",
    "@stepzen/transpiler": "0.0.38",
    "chalk": "^4.1.1",
    "chokidar": "^3.5.2",
    "compare-versions": "^3.6.0",
    "date-fns": "^2.26.0",
    "debug": "^4.3.4",
    "detect-port": "^1.3.0",
    "dotenv": "^8.6.0",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.7",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.6",
    "prettier": "^2.6.2",
    "pretty-ms": "^7.0.1",
    "shelljs": "^0.8.5",
    "throttle-debounce": "^3.0.1",
    "uuid": "^8.3.2",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.7",
    "@types/chai": "^4.2.12",
    "@types/cors": "^2.8.10",
    "@types/cross-spawn": "^6.0.2",
    "@types/debug": "^4.1.6",
    "@types/detect-port": "^1.3.0",
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^9.0.7",
    "@types/inquirer": "^7.3.1",
    "@types/license-checker": "^25.0.1",
    "@types/localtunnel": "^2.0.0",
    "@types/memory-cache": "^0.2.1",
    "@types/minimist": "^1.2.1",
    "@types/mocha": "^5.2.7",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/prettier": "^2.1.6",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.3.9",
    "@types/shelljs": "^0.8.8",
    "@types/sinon-chai": "3.2.8",
    "@types/throttle-debounce": "^2.1.0",
    "@types/unzipper": "^0.10.4",
    "@types/uuid": "^8.3.4",
    "chai": "^4.2.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "7.11.0",
    "eslint-config-oclif": "3.1.0",
    "eslint-config-oclif-typescript": "0.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "^7.0.4",
    "license-checker": "^25.0.1",
    "lint-staged": "^12.3.2",
    "mocha": "^5.2.0",
    "mock-fs": "^4.13.0",
    "nock": "^13.0.4",
    "nodemon": "^2.0.7",
    "nyc": "^14.1.1",
    "semver": "^7.3.7",
    "sinon": "13.0.1",
    "sinon-chai": "3.7.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "stepzen",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "prerun": [
        "./lib/hooks/prerun/check-upgrade",
        "./lib/hooks/prerun/ensure-config-file",
        "./lib/hooks/prerun/ensure-permissions"
      ]
    }
  },
  "scripts": {
    "dev:login": "nodemon -e ts bin/run login",
    "dev:start": "nodemon -e ts bin/run start --dir ~/Downloads/hello-stepzen",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ignore-path .prettierignore",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "tsc -b && cross-env STEPZEN_INTERACTIVE=true STEPZEN_DOMAIN=steprz.io STEPZEN_SERVER_URL=http://localhost nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test:single:example": "npm run test:single -- test/src/shared/curl-parser.test.ts",
    "test:single": "tsc -b && cross-env STEPZEN_INTERACTIVE=true STEPZEN_DOMAIN=steprz.io STEPZEN_SERVER_URL=http://localhost mocha --forbid-only test/_global.test.ts",
    "version": "oclif-dev readme && git add README.md",
    "prepare": "husky install"
  },
  "types": "lib/index.d.ts",
  "lint-staged": {
    "*.{ts,js,css,md,yaml,json}": "prettier --write"
  }
}
